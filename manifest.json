{
  "manifest_version": 2,
  "name": "LostFilm Torrent Button Unlocker",
  "version": "1.4",
  "developer": {
    "name": "Yuriy Nasirov",
    "url": "https://github.com/nasirov/torrent-button-unlocker"
  },
  "description": "Allows you to download torrents from LostFilm site if a content is blocked in your country.",
  "applications": {
    "gecko": {
      "id": "torrentButtonUnlocker@nasirov.yv",
      "strict_min_version": "66.0"
    }
  },
  "content_scripts": [
    {
      "js": [
        "jquery-3.4.0.slim.min.js",
        "unlock.js"
      ],
      "matches": [
        "https://www.lostfilm.tv/series/*/season*",
        "http://www.lostfilm.tv/series/*/season*",
        "https://www.lostfilm.run/series/*/season*",
        "http://www.lostfilm.run/series/*/season*",
        "https://www.lostfilm.uno/series/*/season*",
        "http://www.lostfilm.uno/series/*/season*"
      ],
      "run_at": "document_end"
    }
  ]
}